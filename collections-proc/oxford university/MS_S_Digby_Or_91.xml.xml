                  </msItem>
                  <msItem xml:id="MS_S_Digby_Or_91-item4" n="4">
                     <locus from="7" to="10">ff. 10b-12b</locus>
                     <title xml:lang="fa-Latn-x-lc">Some Persian Quatrains</title>
                     <incipit xml:lang="fa"/>
                     <textLang mainLang="fa">Persian</textLang>
                  </msItem>
                  <msItem xml:id="MS_S_Digby_Or_91-item5" n="5">
                     <locus from="13" to="27">ff. 13a-27a</locus>
                     <editor role="translator"/>
                     <title xml:lang="fa-Latn-x-lc">Risālah-ʼi Radd-i bidʻat</title>
                     <title xml:lang="fa">رسالۀ ردّ بدعت</title>

               <msContents class="#multipleWorks">
                  <summary>A collection of risālahs in poetry and prose, composed 1869 AD.</summary>
                  <msItem xml:id="MS_S_Digby_Or_91-item1" n="1">
                     <locus from="1" to="3">ff. 1-3</locus>
                     <title xml:lang="fa-Latn-x-lc">Risālah-ʼi Jihādīyah</title>
                     <title xml:lang="fa">رسالۀ جهادیه</title>
                     <note>An Urdu mathnavī.</note>
                     <incipit xml:lang="fa"/>
                     <textLang mainLang="ur">Urdu</textLang>
                  </msItem>
                  <msItem xml:id="MS_S_Digby_Or_91-item2" n="2">
                     <locus from="4" to="7">ff. 4-7a</locus>
                     <title xml:lang="fa-Latn-x-lc">Risālah-ʼi Jihādīyah</title>
                     <title xml:lang="fa">رسالۀ جهادیه</title>
                     <note>Another version (in a different hand) of the previous mathnavī.</note>
                     <incipit xml:lang="fa"/>
                     <textLang mainLang="ur">Urdu</textLang>
                  </msItem>
                  <msItem xml:id="MS_S_Digby_Or_91-item3" n="3">
                     <locus from="7" to="10">ff. 7b-10a</locus>
                     <title xml:lang="fa-Latn-x-lc">Risālah-ʼi sad u sih masʻalah</title>
                     <title xml:lang="fa">رسالۀ سد و سه مسأله</title>
                     <incipit xml:lang="fa"/>

                            <author key="person_f8546">
                                <persName xml:lang="ota-Latn-x-lc">Şeyḫ Ḥüseyn bin el-Ḥācc Ḥasan Aġa</persName> <persName xml:lang="ota">شیخ حسین بن الحج حسن آغا</persName>

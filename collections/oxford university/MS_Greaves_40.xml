               <msContents class="#multipleWorks">
                  <summary change="#add-summary2"> 5 works by 2 authors on the subjects of Universal
                     history, Astronomy, Christian writings, Divination, and Geomancy </summary>
                  <msItem xml:id="MS_Greaves_40-item1" n="1">
                     <locus from="4" to="39">ff. 4r-39v</locus>
                     <author key="person_f407">Anonymous</author>
                     <title key="work_3366">A compendium of history from Adam to the caliphs</title>
                     <note>ff. 1-2 fragment of a horoscope, followed by a charm f. 3r bears the
                        words <foreign xml:lang="ar-Latn-x-lc">Kitab al-taʼrīkh</foreign>, otherwise
                        the leaf is blank</note>
                     <listBibl>
                        <bibl>Appendix History</bibl>
                        <bibl>[UAM. 863 (1)]</bibl>
                     </listBibl>
                     <textLang mainLang="ar">Arabic</textLang>
                  </msItem>
                  <msItem xml:id="MS_Greaves_40-item2" n="2">
                     <locus from="40" to="59">ff. 40r-59v</locus>
                     <author key="person_56429565" ref="http://viaf.org/viaf/56429565/">
                        <persName type="standard" xml:lang="ar-Latn-x-lc">Māridīnī, ʻAbd Allāh
                           ibn Khalīl, <date calendar="#Gregorian" when="1406">d. 1406</date>
                        </persName>
                     </author>
                     <title xml:lang="ar-Latn-x-lc" key="work_3367">Risālah mukhtaṣarah fī al-ʻamal
                        bi-rubʻ al-dāʼirah</title>
                     <listBibl>
                        <bibl>GAL II 169</bibl>
                        <bibl>[UAM. 863 (2)]</bibl>
                     </listBibl>
                     <textLang mainLang="ar">Arabic</textLang>
                  </msItem>
                  <msItem xml:id="MS_Greaves_40-item3" n="3">
                     <locus from="60" to="113">ff. 60r-113v</locus>
                     <author key="person_f407">Anonymous</author>
                     <title key="work_3368">A work on the lengthening and shortening of the days,
                        the mansions of the moon, and the planets</title>
                     <listBibl>
                        <bibl>Appendix Astronomy</bibl>
                        <bibl>[UAM. 863 (3)]</bibl>
                     </listBibl>
                     <textLang mainLang="ar">Arabic</textLang>
                  </msItem>
                  <msItem xml:id="MS_Greaves_40-item4" n="4">
                     <locus from="114" to="154">ff. 114r-154v</locus>
                     <author key="person_f407">Anonymous</author>
                     <title key="work_3369">A fragment about the debate of <persName key="person_f3603" role="subject" change="#role-assignment">Saint
                           Anbabula</persName> [sic] with the devil</title>
                     <listBibl>
                        <bibl>Christian Appendix Sermons and Theological Treatises</bibl>
                        <bibl>[UAM. 863 (4)]</bibl>
                     </listBibl>
                     <textLang mainLang="ar">Arabic</textLang>
                  </msItem>
                  <msItem xml:id="MS_Greaves_40-item5" n="5">
                     <locus from="135" to="221">ff. 135r-221v</locus>

               <msContents class="#multipleWorks">
                  <summary change="#add-summary2">
                     3 works by 2 authors on the subjects of Psalters, Islamic magic, and Islam
                  </summary>
                  <msItem xml:id="MS_Bodl_Or_429-item1" n="1">
                     <locus from="1" to="64">ff. 1v-64v</locus>
                     <title key="work_3834">Pseudo-Davidic Psalms in Arabic translation</title>
                     <note>Transcribed from MS. Huntington 515 by <persName key="person_f1308" role="scr" change="#role-assignment">Henry Wild of Norwich</persName>
                     </note>
                     <note>Pss. 60-143</note>
                     <listBibl>
                        <bibl>Christian Appendix Hymnology</bibl>
                        <bibl>[NAM. 50 (1)]</bibl>
                     </listBibl>
                     <textLang mainLang="ar">Arabic</textLang>
                  </msItem>
                  <msItem xml:id="MS_Bodl_Or_429-item2" n="2" class="#extract">
                     <locus from="64" to="66">ff. 64v-66r</locus>
                     <author key="person_f407">Anonymous</author>
                     <title key="work_3835">An extract giving twelve verses said by " <persName type="standard" xml:lang="ar-Latn-x-lc" key="person_155546527" ref="http://viaf.org/viaf/155546527/" role="ant" change="#role-assignment #ids-cleanup">Kaʻb al-Aḥbār, <date calendar="#Gregorian" when="0656">d. 655 or 6</date>
                        </persName> " to be of magic and prophylactic properties</title>
                     <note>Transcribed from MS. Huntington 515 by <persName key="person_f1308" role="scr" change="#role-assignment">Henry Wild of Norwich</persName>
                     </note>
                     <listBibl>
                        <bibl>Appendix Magic</bibl>
                        <bibl>[NAM. 50 (2)]</bibl>
                     </listBibl>
                     <textLang mainLang="ar">Arabic</textLang>
                  </msItem>
                  <msItem xml:id="MS_Bodl_Or_429-item3" n="3">

                  <msItem xml:id="MS_Huntington_363-item3" n="3">
                     <locus from="22" to="33">ff. 22v-33v</locus>
                     <author key="person_81971228" ref="http://www.viaf.org/viaf/81971228/">
                        <persName type="standard" xml:lang="ar-Latn-x-lc">Zayn al-ʻĀbidīn ʻAlī ibn al-Ḥusayn, d. <date calendar="#Gregorian" when="0710" cert="unknown">710</date>?</persName>
                     </author>
                     <title xml:lang="ar-Latn-x-lc" key="work_47">al-Ṣaḥīfah al-kāmilah</title>
                     <title type="alt" xml:lang="ar-Latn-x-lc" key="work_47">al-Ṣaḥīfah al-Sajjādīyah</title>
                     <note>First part only including the introduction</note>
                     <listBibl>
                        <bibl>GAL I 44</bibl>
                        <bibl>[UAM. 339 (3)]</bibl>
                     </listBibl>
                     <textLang mainLang="ar">Arabic</textLang>

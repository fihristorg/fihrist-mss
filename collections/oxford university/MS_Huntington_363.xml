                     <author key="person_88873163" change="#ids-cleanup">
                        <persName>Hippolytus of Rome <date calendar="#Gregorian" when="0236">d.c. 236</date>
                        </persName>
                     </author>
                     <title key="work_4027">Brief commentary on the Pentateuch</title>
                     <note>Lacking beginning and end</note>
                     <listBibl>
                        <bibl>Graf I, p. 307</bibl>
                        <bibl>[UAM. 339 (2)]</bibl>
                     </listBibl>
                     <textLang mainLang="ar">Arabic</textLang>

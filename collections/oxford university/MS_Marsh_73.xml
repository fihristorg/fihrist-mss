               <msContents class="#multipleWorks">
                  <summary change="#add-summary2">
                     2 works by 2 authors on the subject of Arabic poetry
                  </summary>
                  <msItem xml:id="MS_Marsh_73-item1" n="1">
                     <locus from="1" to="91">ff. 1-91v</locus>
                     <author key="person_f4760">
                        <persName xml:lang="ar-Latn-x-lc">ʻAlā al-Dīn al-Ṣāliḥī al-Ḥarīrī</persName>
                     </author>
                     <title xml:lang="ar-Latn-x-lc" key="work_4762">al-Durr al-maṣūn fī niẓām al-sabʻ funūn</title>
                     <note>Incomplete; several lacunae and lacking end</note>
                     <note>f. (i) notes and verses in a later hand</note>
                     <listBibl>
                        <bibl>Appendix Poetry</bibl>
                        <bibl>[UAM. 1294, with corrig. Nicoll p. 618, plus 43 fols. (ff. 49-91) formerly MS. Arab. f. 8 and MS. Arab. f. 10 combined [= K 290] and now restored]</bibl>
                     </listBibl>
                     <textLang mainLang="ar">Arabic</textLang>
                  </msItem>
                  <msItem xml:id="MS_Marsh_73-item2" n="2">
                     <locus from="92" to="95">ff. 92r-95v</locus>
                     <author key="person_68941158" ref="http://www.viaf.org/viaf/68941158/">
                        <persName type="standard" xml:lang="ar-Latn-x-lc" xml:id="m2">Abū al-ʻAlāʼ al-Maʻarrī, <date calendar="#Gregorian" from="0973" to="1057">973-1057</date>
                        </persName>
                     </author>
                     <title xml:lang="ar-Latn-x-lc" key="work_1494">Saqṭ al-zand</title>
                     <note>2 fragments</note>
                     <note>f. 96 notes and verses in a later hand</note>
                     <listBibl>
                        <bibl>GAL I 255</bibl>
                        <bibl>[UAM. 1294 (this item not mentioned); formerly MS. Arab. f. 11 [= K 292] and now restored]</bibl>
                     </listBibl>
                     <textLang mainLang="ar">Arabic</textLang>
                  </msItem>

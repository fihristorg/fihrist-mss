                        <persName xml:lang="ar-Latn-x-lc" type="standard">anonymous</persName>
                     </author>
                     <title xml:lang="en" type="desc" key="work_20522" change="#ids-cleanup">Anthology</title>
                     <incipit xml:lang="fa">ز هستیت چیست اگر نیستی دلا</incipit>
                     <explicit xml:lang="fa">که هرچه غرق شد آن بکار نمی آمد</explicit>

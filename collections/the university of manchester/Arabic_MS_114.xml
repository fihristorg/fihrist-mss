                            <title xml:lang="ar">شاهنامه</title>
                            <title xml:lang="en" type="supplied"/>
                            <incipit xml:lang="ar">جهاندار ضحاك</incipit>
                                                       <textLang mainLang="fa">Persian</textLang>
                        </msItem>
                        <msItem xml:id="manuscript_13814-part9">
                            <locus from="58b" to="61a">Folios 58b-61a</locus>
                                                        <note>Anonymous poetical extracts from the Mas̲navī of Jalāl al-Dīn Rūmī.</note>
                            <author key="person_98930150" ref="http://viaf.org/viaf/98930150">
                                <persName xml:lang="ar-Latn-x-lc" type="standard">Jalāl al-Dīn Rūmī</persName>
                                <persName xml:lang="ar" type="org">جلال الدين الرومي</persName>
                            </author>
                            <title xml:lang="ar-Latn-x-lc" key="work_20506" change="#ids-cleanup">Mas̲navī</title>
                            <title xml:lang="ar">مثنوى</title>
                            <textLang mainLang="fa">Persian</textLang>
                        </msItem>
                        <msItem xml:id="manuscript_13814-part10">
                            <locus from="61b" to="64a">Folios 61b-64a</locus>
                                                        <note>Two poems, said to be by Aḥmad Hātif Iṣfahānī. </note>
                            <author key="person_9100689" ref="http://viaf.org/viaf/9100689">
                                <persName xml:lang="ar-Latn-x-lc" type="standard">Aḥmad Hātif Iṣfahānī</persName>
                                <persName xml:lang="ar" type="org">احمد هاتف اصفهانی</persName>
                            </author>
                            <incipit xml:lang="ar">اى فداي</incipit>
                            <explicit xml:lang="ar"/>
                            <textLang mainLang="fa">Persian</textLang>
                        </msItem>
                        <msItem xml:id="manuscript_13814-part11">
                            <locus from="64b" to="65a">Folios 64b-65a</locus>
                                                        <note>A Ghazal by Abū Ṭālib Khān, in honour of Miss Rickets. </note>
                            <author key="person_20819905" ref="http://viaf.org/viaf/20819905">
                                <persName xml:lang="ar-Latn-x-lc" type="standard">Abū Ṭālib Khān</persName>
                                <persName xml:lang="ar" type="org">أبو طالب خان</persName>
                            </author>
                            <textLang mainLang="fa">Persian</textLang>
                        </msItem>
                        <msItem xml:id="manuscript_13814-part12">
                            <locus from="65b" to="70a">Folios 65b-70</locus>
                            
                            <note>Further extracts from the Nigāristān of Kamal Pasha Zadah (from bābs 2 and 3). </note>
                            <author key="person_53153025" ref="http://viaf.org/viaf/53153025">
                                <persName xml:lang="ar-Latn-x-lc" type="standard">Kemalpaşazade</persName>
                                <persName xml:lang="ar" type="org">كمالپشزاده</persName>
                            </author>
                            <title xml:lang="ar-Latn-x-lc" key="work_20588" change="#ids-cleanup">Nigāristān</title>
                            <title xml:lang="ar">نگارستان</title>
                                                        <textLang mainLang="fa">Persian</textLang>
                        </msItem>
                        <msItem xml:id="manuscript_13814-part13">
                            <locus from="70">Folios 70b</locus>
                                                        <note>A couplet from the Humāyūn Nāmah</note>
                                            <textLang mainLang="fa">Persian</textLang>
                        </msItem>
                        <msItem xml:id="manuscript_13814-part14">
                            <locus from="71">Folios 71</locus>
                                                        <note>A Ghazal, in Turkish, possibly by Rifʿat Efendi</note>
                           <author key="person_f6001">
                                <persName xml:lang="ar-Latn-x-lc" type="standard">Rifʿat Efendi</persName>
                                <persName xml:lang="ar" type="org">رفعت افندى</persName>
                            </author>
                            <title xml:lang="ar-Latn-x-lc" key="work_11247">Ghazal Sulṭān Salīm Takhmīs Rifʿat</title>

                                        <lb/><dimensions type="illustration" unit="mm">
                                            <height>165</height>
                                            <width>102</width>
                                        </dimensions>
                                            <lb/>
                                            Published: Robinson, <title>Persian Paintings</title>, p. 246.
                                    </item>

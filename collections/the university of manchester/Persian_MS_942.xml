                                    <persName xml:lang="fa">محمد یوسف الحسینی</persName>
                                </author>
                                <title xml:lang="fa-Latn-x-lc" key="work_723">Kitāb-i Taṣrīf</title>
                                <textLang mainLang="fa">Persian</textLang>

                     <author key="person_f407" change="#ids-cleanup">
                        <persName xml:lang="fa-Latn-x-lc" type="supplied"/>Anonymous</author>
                     <title xml:lang="fa-Latn-x-lc" key="work_8596">Savāniḥāt-i Afyāl</title>
                     <title xml:lang="fa" key="work_8596">سوانحات افیال</title>

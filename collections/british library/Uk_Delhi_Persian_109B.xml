                     </listBibl>
                     <textLang mainLang="fa">Persian</textLang>
                     <note>
                  </note>
                  </msItem>
                  <msItem xml:id="Uk_Delhi_Persian_109B-item2" n="2" class="http://lccn.loc.gov/sh2010102954 http://lccn.loc.gov/sh85024485">
                     <locus from="25v" to="58v">ff. 25v, 58v</locus>
                     <author key="person_f407" change="#ids-cleanup">
                        <persName xml:lang="en" type="desc">Unknown</persName>

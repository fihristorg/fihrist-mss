                        <persName xml:lang="ps">شیدا، کاظم خان</persName>
                     </author>
                     <title xml:lang="pus-Latn-x-lc" key="work_7795">Dīwān-i Shaydā</title>
                     <title xml:lang="ps" key="work_7795">دیوان شیدا</title>
